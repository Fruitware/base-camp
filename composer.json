{
  "name": "suomato/base-camp",
  "description": "WordPress starter theme for developer based on Timber-library",
  "license": "MIT",
  "type":"wordpress-theme",
  "minimum-stability" : "stable",
  "authors": [
    {
      "name": "Toni Suomalainen",
      "email": "toni.suomalainen@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "extra": {
    "installer-paths": {
      "../../plugins/{$name}/": [
        "wpackagist-plugin/*",
        "type:wordpress-plugin"
      ]
    }
  },
  "require": {
    "wpackagist-plugin/timber-library": "1.3.4",
    "wpackagist-plugin/advanced-custom-fields": "4.4.11",
    "symfony/var-dumper": "^3.3"
  }
}
